  "bugs": {
    "url": "https://github.com/markfaulk350/TypeScript-Express/issues"
  },
  "homepage": "https://github.com/markfaulk350/TypeScript-Express#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^13.13.5",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3",
    "typescript": "^3.8.3"
  }
