{
  "name": "TypeScript-Express",
  "version": "1.0.0",
  "description": "The purpose of this project is to create a simple example on how to setup an express API using TypeScript. There are tons of tutorials and articles out there but none of them are complete or consistent.",
  "main": "index.js",
  "scripts": {
    "start:build": "tsc -w",
    "start:run": "nodemon dist/index.js",
    "start": "concurrently npm:start:*",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markfaulk350/TypeScript-Express.git"
  },
  "keywords": [],
